{
    "manifest_version": 2,
    "name": "Enhanced Master Vault",
    "short_name": "Enhanced Master Vault",
    "version": "0.1",
    "permissions": [
        "activeTab",
        "cookies",
        "declarativeContent",
        "storage",
        "https://www.keyforgegame.com/"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://www.keyforgegame.com/*"
            ],
            "js": [
                "arrive.min.js",
                "enhanced_master_vault.js"
            ],
            "css": [
                "pure-css-drawer-menu.css"
            ]
        }
    ],
    "web_accessible_resources": [
        "dokicon-16x16.png"
    ],
    "icons": {
        "100": "icon.png"
    }
}